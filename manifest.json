{
  "manifest_version": 2,
  "name": "Custon New Tab for Edge",
  "version": "0.1.0",
  "description": "Use html file as new tab",
  "applications": {
    "gecko": {
      "id": "mardevour@github",
      "strict_min_version": "78.0"
    }
  },
  "permissions": ["storage", "tabs"],
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  }
}
